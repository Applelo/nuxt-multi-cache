{
  "name": "nuxt-multi-cache",
  "version": "1.0.3",
  "description": "SSR route, component and data cache for Nuxt.js",
  "keywords": [
    "nuxt",
    "cache",
    "ssr",
    "page",
    "caching",
    "component"
  ],
  "author": {
    "name": "Jan Hug",
    "url": "https://dulnan.net",
    "email": "me@dulnan.net"
  },
  "scripts": {
    "build": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "typedoc": "typedoc --out foo --namedAnchors true --publicPath /configuration/ src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dulnan/nuxt-multi-cache.git"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "browser": {
    "./index.js": "./client.js"
  },
  "files": [
    "lib/",
    "client.js",
    "client.d.ts"
  ],
  "dependencies": {
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "lru-cache": "^6.0.0",
    "make-dir": "^3.1.0",
    "mkdirp": "^1.0.4",
    "sqlite3": "^5.0.2",
    "ufo": "^0.7.4"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.6",
    "@types/basic-auth": "^1.1.3",
    "@types/lowdb": "^1.0.9",
    "@types/lru-cache": "^5.1.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^15.3.0",
    "@types/sqlite3": "^3.1.7",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^11.0.3",
    "nuxt": "^2.15.6",
    "prettier": "^2.3.0",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4",
    "vue": "^2.6.12",
    "vue-server-renderer": "^2.6.12"
  }
}
