{
  "name": "nuxt-multi-cache",
  "version": "2.0.0",
  "description": "SSR route, component and data cache for Nuxt.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "keywords": [
    "nuxt",
    "cache",
    "ssr",
    "page",
    "caching",
    "component"
  ],
  "author": {
    "name": "Jan Hug",
    "url": "https://dulnan.net",
    "email": "me@dulnan.net"
  },
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "typedoc": "typedoc --out foo --namedAnchors true --publicPath /configuration/ src/index.ts",
    "prettier": "prettier ./src --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dulnan/nuxt-multi-cache.git"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/kit": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.29.0",
    "nuxt": "^3.0.0",
    "prettier": "^2.8.1"
  }
}
